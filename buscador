def buscador (arr,ele):
    indice=0
    for x in (arr):
        if (ele==x):
            return x
        else:
            indice+1
    return -1

def mensaje(ele):
    if ( ele== -1):
        print("El elemento|Uno no se encuentra en el arreglo")
    else:
        print("El elementoDos se encuentra en el arreglo")

resultadoDos=buscador(arr,elementoUno)
mensaje(resultadoDos)
resultadoUno=buscador(arr,elementoDos)
mensaje(resultadoUno),print("En el indice:" , buscador(arr,elementoDos))
